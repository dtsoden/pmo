{
  "name": "pmo-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend API for PMO Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc && node --watch dist/index.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "studio": "prisma studio",
    "generate": "prisma generate",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed:test": "npx ts-node scripts/seed-test-data.ts",
    "seed:clear": "npx ts-node scripts/clear-test-data.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "^5.10.2",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/archiver": "^7.0.0",
    "archiver": "^7.0.1",
    "bcrypt": "^5.1.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "redis": "^5.10.0",
    "socket.io": "^4.7.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
